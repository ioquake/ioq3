seta sv_hostname "Quake III"
seta sv_maxclients 16
seta g_motd "ioquake"
seta g_quadfactor 4
seta g_gametype 0
seta timelimit 15
seta fraglimit 25
seta g_weaponrespawn 3
seta g_inactivity 3000
seta g_forcerespawn 0
seta rconpassword "ioquake3" // Change this!


//#### MAPS ####
set d1 "map q3dm1 ; set nextmap vstr d2"
set d2 "map q3dm2 ; set nextmap vstr d3"
set d3 "map q3dm3 ; set nextmap vstr d1"
vstr d1


//#### BOTS ####
seta bot_enable 1       // Allow bots on the server
//seta bot_nochat 1       // Shut those fucking bots up
seta g_spskill 4        // Default skill of bots [1-5] 
seta bot_minplayers 5   // This fills the server with bots to satisfy the minimum

